{
  "name": "awesome-notes",
  "version": "3.5.4",
  "description": "Personal Learning Notes (TIL)",
  "main": "README.md",
  "scripts": {
    "build": "docusaurus build",
    "clear": "docusaurus clear",
    "dev": "docusaurus start",
    "format": "prettier --write notes/**/*.{md,mdx}",
    "lint": "yarn lint:notes && yarn lint:style && yarn lint:type-check",
    "lint:notes": "markdownlint -i node_modules ./notes && stylelint ./notes/**/*.md",
    "lint:style": "eslint --ext .ts,.tsx ./src && stylelint ./src/**/*.css",
    "lint:type-check": "tsc --noEmit",
    "prelease": "standard-version --dry-run -s",
    "release": "standard-version -s",
    "serve": "docusaurus serve",
    "start": "yarn dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/awesome-notes.git"
  },
  "keywords": [
    "CS",
    "Notes",
    "Docusaurus",
    "GitBook",
    "HTML",
    "CSS",
    "JavaScript",
    "TypeScript",
    "React",
    "Web",
    "TIL"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/awesome-notes/issues"
  },
  "homepage": "https://github.com/sabertazimi/awesome-notes#readme",
  "packageManager": "yarn@3.0.2",
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.7",
    "@docusaurus/preset-classic": "2.0.0-beta.7",
    "@easyops-cn/docusaurus-search-local": "^0.19.1",
    "@mdx-js/react": "^1.6.22",
    "@svgr/webpack": "^5.5.0",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "nodejieba": "^2.5.2",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.5.2",
    "@dg-scripts/stylelint-config": "^5.5.2",
    "@docusaurus/module-type-aliases": "2.0.0-beta.7",
    "@tsconfig/docusaurus": "^1.0.4",
    "@types/react": "^17.0.27",
    "@types/react-helmet": "^6.1.3",
    "@types/react-router-dom": "^5.3.1",
    "eslint": "^7.32.0",
    "markdownlint-cli": "^0.29.0",
    "prettier": "^2.4.1",
    "standard-version": "^9.3.1",
    "stylelint": "^13.13.1",
    "typescript": "^4.4.3",
    "webpack": "^5.58.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
