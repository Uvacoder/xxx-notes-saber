{
  "name": "awesome-notes",
  "version": "3.5.6",
  "description": "Personal Learning Notes (TIL)",
  "main": "README.md",
  "scripts": {
    "build": "docusaurus build",
    "clear": "docusaurus clear",
    "dev": "docusaurus start",
    "format": "prettier --write notes/**/*.{md,mdx}",
    "lint": "yarn lint:notes && yarn lint:style && yarn lint:type-check",
    "lint:fix": "yarn lint:notes:fix && yarn lint:style:fix",
    "lint:notes": "markdownlint -i node_modules ./notes",
    "lint:notes:fix": "markdownlint -i node_modules ./notes --fix",
    "lint:style": "eslint --ext .ts,.tsx ./src && stylelint ./src/**/*.css",
    "lint:style:fix": "eslint --ext .ts,.tsx ./src --fix && stylelint ./src/**/*.css --fix",
    "lint:type-check": "tsc --noEmit",
    "prelease": "standard-version --dry-run -s",
    "release": "standard-version -s",
    "serve": "docusaurus serve",
    "start": "yarn dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/awesome-notes.git"
  },
  "keywords": [
    "CS",
    "Notes",
    "Docusaurus",
    "GitBook",
    "HTML",
    "CSS",
    "JavaScript",
    "TypeScript",
    "React",
    "Web",
    "TIL"
  ],
  "author": "sabertazimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sabertazimi/awesome-notes/issues"
  },
  "homepage": "https://github.com/sabertazimi/awesome-notes#readme",
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.16",
    "@docusaurus/preset-classic": "2.0.0-beta.16",
    "@easyops-cn/docusaurus-search-local": "^0.21.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.6.2",
    "@dg-scripts/stylelint-config": "^5.6.2",
    "@docusaurus/module-type-aliases": "2.0.0-beta.16",
    "@tsconfig/docusaurus": "^1.0.4",
    "eslint": "^8.10.0",
    "markdownlint-cli": "^0.31.1",
    "nodejieba": "^2.6.0",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "stylelint": "^14.5.3",
    "typescript": "^4.5.5"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
